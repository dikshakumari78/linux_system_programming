#include <iostream>
#include <unistd.h>
#include <sys/wait.h>

int main() {
    pid_t pid = fork();

    if (pid < 0) {
        perror("fork failed");
        return 1;
    } else if (pid == 0) {
        std::cout << "Child Process" << std::endl;
    } else {
        // Parent process
        wait(nullptr); 
        std::cout << "Parent Process" << std::endl;
    }

    return 0;
}
